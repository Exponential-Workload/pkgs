{
  "name": "@3xpo/combgen",
  "version": "0.2.5",
  "description": "Generating combinations of all possible values in an object",
  "source": "src/main.ts",
  "main": "dist/main.cjs",
  "module": "dist/main.mjs",
  "types": "dist/main.d.ts",
  "typings": "dist/main.d.ts",
  "scripts": {
    "build": "concurrently \"tsc --emitDeclarationOnly -p ./tsconfig.json \" \"esbuild src/main.ts --outfile=dist/main.mjs --format=esm --platform=neutral --minify --bundle --packages=external --sourcemap\" \"esbuild src/main.ts --outfile=dist/main.cjs --format=cjs --platform=neutral --minify --bundle --packages=external --sourcemap\"",
    "dev": "nodemon --watch src -e ts,json,env --delay 50ms --exec \"npm run build\"",
    "prepublishOnly": "pnpm build",
    "test": "jest"
  },
  "author": "Expo",
  "contributors": [
    {
      "name": "Expo",
      "url": "https://codeberg.org/Expo",
      "email": "expo@expo.moe"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://codeberg.org/Expo/devtools.git",
    "directory": "packages/@3xpo/combgen/"
  },
  "homepage": "https://codeberg.org/Expo/devtools/src/branch/senpai/packages/@3xpo/combgen/",
  "bugs": {
    "url": "https://codeberg.org/Expo/devtools/issues",
    "email": "expo@expo.moe"
  },
  "license": "MIT",
  "engines": {
    "node": ">=v20.0.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.mjs",
      "require": "./dist/main.cjs",
      "node": "./dist/main.mjs",
      "default": "./dist/main.mjs"
    }
  },
  "dependencies": {
    "@3xpo/argparser": "^1.1.2"
  },
  "files": [
    "dist/**/*",
    "dist/*",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "maintainers": [
    {
      "name": "Expo",
      "url": "https://codeberg.org/Expo",
      "email": "expo@expo.moe"
    }
  ],
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.11",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1"
  }
}
