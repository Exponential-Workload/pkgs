{
  "name": "@3xpo/promi",
  "version": "0.1.5",
  "description": "Promisify a callback function",
  "scripts": {
    "dev": "nodemon --watch src -e ts,json,env --delay 50ms --exec \"npm run build && npm run test\"",
    "build": "concurrently \"npm:build:*\"",
    "build:ts": "tsc --emitDeclarationOnly",
    "build:js": "esmany --args \"\\\"src/lib.ts\\\" --sourcemap --packages=external\" --platform neutral --format esm,cjs --bundle true --output dist/lib",
    "test": "jest"
  },
  "author": "Expo",
  "contributors": [
    {
      "name": "Expo",
      "url": "https://codeberg.org/Expo",
      "email": "expo@expo.moe"
    }
  ],
  "maintainers": [
    {
      "name": "Expo",
      "url": "https://codeberg.org/Expo",
      "email": "expo@expo.moe"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://codeberg.org/Expo/pkgs.git",
    "directory": "packages/@3xpo/promi/"
  },
  "homepage": "https://codeberg.org/Expo/pkgs/src/branch/senpai/packages/@3xpo/promi/",
  "bugs": {
    "url": "https://codeberg.org/Expo/pkgs/issues",
    "email": "expo@expo.moe"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/**/*",
    "dist/*",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "source": "./src/lib.ts",
  "main": "./dist/lib.bundle.min.cjs",
  "module": "./dist/lib.bundle.min.mjs",
  "types": "./dist/lib.d.ts",
  "typings": "./dist/lib.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.bundle.min.mjs",
      "require": "./dist/lib.bundle.min.cjs",
      "node": "./dist/lib.bundle.min.mjs",
      "default": "./dist/lib.bundle.min.mjs"
    }
  },
  "displayName": "Promi",
  "engines": {
    "node": ">=v20.0.0"
  },
  "devDependencies": {
    "@3xpo/es-many": "workspace:^",
    "@types/jest": "^29.5.12",
    "concurrently": "^8.2.2",
    "esbuild": "^0.20.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.2"
  }
}
