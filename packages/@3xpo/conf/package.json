{
  "name": "@3xpo/conf",
  "version": "0.1.0",
  "description": "A Configuration Library",
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "dev": "nodemon --watch src -e ts,json,env --delay 50ms --exec \"npm run build && npm run test\"",
    "build:ts": "tsc --emitDeclarationOnly",
    "build:js": "esmany --args \"\\\"src/lib.ts\\\" --sourcemap --packages=external\" --platform node --format esm,cjs --bundle true --output dist/lib",
    "test": "jest"
  },
  "author": "Expo",
  "contributors": [
    {
      "name": "Expo",
      "url": "https://codeberg.org/Expo",
      "email": "expo@expo.moe"
    }
  ],
  "maintainers": [
    {
      "name": "Expo",
      "url": "https://codeberg.org/Expo",
      "email": "expo@expo.moe"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://codeberg.org/Expo/devtools.git",
    "directory": "packages/@3xpo/conf/"
  },
  "homepage": "https://codeberg.org/Expo/devtools/src/branch/senpai/packages/@3xpo/conf/",
  "bugs": {
    "url": "https://codeberg.org/Expo/devtools/issues",
    "email": "expo@expo.moe"
  },
  "license": "MIT",
  "engines": {
    "node": ">=v20.0.0"
  },
  "type": "module",
  "files": [
    "dist/**/*",
    "dist/*",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "source": "./src/lib.ts",
  "types": "./dist/lib.d.ts",
  "main": "./dist/lib.node.bundle.min.cjs",
  "module": "./dist/lib.node.bundle.min.mjs",
  "typings": "./dist/lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib.node.bundle.min.mjs",
      "require": "./dist/lib.node.bundle.min.cjs",
      "types": "./dist/lib.d.ts",
      "node": "./dist/lib.node.bundle.min.mjs",
      "default": "./dist/lib.node.bundle.min.mjs"
    }
  },
  "displayName": "conf",
  "dependencies": {
    "@3xpo/events": "^1.0.7"
  },
  "devDependencies": {
    "@3xpo/es-many": "workspace:^",
    "@types/jest": "^29.5.11",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.11",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1"
  },
  "optionalDependencies": {
    "zod": "^3.22.4"
  }
}
