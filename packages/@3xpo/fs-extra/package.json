{
  "name": "@3xpo/fs-extra",
  "version": "11.2.2",
  "description": "fs-extra contains methods that aren't included in the vanilla Node.js fs package. Such as recursive mkdir, copy, and remove.",
  "engines": {
    "node": ">=14.14"
  },
  "keywords": [
    "fs",
    "file",
    "file system",
    "copy",
    "directory",
    "extra",
    "mkdirp",
    "mkdir",
    "mkdirs",
    "recursive",
    "json",
    "read",
    "write",
    "extra",
    "delete",
    "remove",
    "touch",
    "create",
    "text",
    "output",
    "move",
    "promise"
  ],
  "author": "Expo",
  "license": "MIT",
  "dependencies": {
    "@3xpo/universalify": "workspace:^",
    "graceful-fs": "^4.2.0"
  },
  "main": "./dist/lib.node.bundle.min.cjs",
  "module": "./dist/lib.node.bundle.min.mjs",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/lib.node.bundle.min.mjs",
      "require": "./dist/lib.node.bundle.min.cjs",
      "node": "./dist/lib.node.bundle.min.mjs",
      "default": "./dist/lib.node.bundle.min.mjs"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "build:dts": "tsc --emitDeclarationOnly --declaration",
    "build:js": "es-many --args \"src/index.ts --sourcemap --packages=external\" --output dist/lib --minify true --bundle true --format cjs,esm --platform node",
    "test-find": "find ./dist-test/**/__tests__ -name *.test.js | xargs mocha",
    "test": "jest"
  },
  "sideEffects": false,
  "contributors": [
    {
      "name": "Expo",
      "url": "https://codeberg.org/Expo",
      "email": "expo@expo.moe"
    }
  ],
  "maintainers": [
    {
      "name": "Expo",
      "url": "https://codeberg.org/Expo",
      "email": "expo@expo.moe"
    }
  ],
  "homepage": "https://codeberg.org/Expo/devtools/src/branch/senpai/packages/@3xpo/fs-extra/",
  "repository": {
    "type": "git",
    "url": "https://codeberg.org/Expo/devtools.git",
    "directory": "packages/@3xpo/fs-extra/"
  },
  "bugs": {
    "url": "https://codeberg.org/Expo/devtools/issues",
    "email": "expo@expo.moe"
  },
  "type": "module",
  "devDependencies": {
    "@3xpo/es-many": "workspace:^",
    "@types/graceful-fs": "^4.1.9",
    "@types/universalify": "^1.0.3",
    "jest": "^29.7.0",
    "klaw": "^2.1.1",
    "klaw-sync": "^3.0.2",
    "minimist": "^1.1.1",
    "mocha": "^10.1.0",
    "nyc": "^15.0.0",
    "proxyquire": "^2.0.1",
    "read-dir-files": "^0.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
