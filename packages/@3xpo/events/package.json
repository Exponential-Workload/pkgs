{
  "name": "@3xpo/events",
  "version": "1.0.7",
  "description": "NodeJS EventEmitter Polyfill in TS, with event type support",
  "main": "dist/lib.bundle.min.cjs",
  "module": "dist/lib.bundle.min.mjs",
  "types": "dist/lib.d.ts",
  "typings": "dist/lib.d.ts",
  "exports": {
    "types": "./dist/lib.d.ts",
    "import": "./dist/lib.bundle.min.mjs",
    "require": "./dist/lib.bundle.min.cjs",
    "node": "./dist/lib.bundle.min.mjs",
    "default": "./dist/lib.bundle.min.mjs"
  },
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "build:dts": "tsc --declaration --emitDeclarationOnly --declarationMap",
    "build:js": "es-many --args \"src/lib.ts --sourcemap --packages=external\" --output dist/lib --minify true --bundle true --format cjs,esm --platform neutral",
    "prepublish": "pnpm build",
    "mkdocs": "typedoc src",
    "mkdoc": "typedoc src",
    "test": "jest"
  },
  "keywords": [],
  "author": "Expo",
  "license": "MIT",
  "homepage": "https://codeberg.org/Expo/devtools/src/branch/senpai/packages/@3xpo/events/",
  "repository": {
    "type": "git",
    "url": "https://codeberg.org/Expo/devtools.git",
    "directory": "packages/@3xpo/events/"
  },
  "contributors": [
    {
      "name": "Expo",
      "url": "https://codeberg.org/Expo",
      "email": "expo@expo.moe"
    }
  ],
  "maintainers": [
    {
      "name": "Expo",
      "url": "https://codeberg.org/Expo",
      "email": "expo@expo.moe"
    }
  ],
  "bugs": {
    "url": "https://codeberg.org/Expo/devtools/issues",
    "email": "expo@expo.moe"
  },
  "engines": {
    "node": ">=v20.0.0"
  },
  "type": "module",
  "files": [
    "dist/**/*",
    "dist/*",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "dependencies": {},
  "devDependencies": {
    "@3xpo/es-many": "workspace:^",
    "@types/jest": "^29.5.2",
    "concurrently": "^8.2.2",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.3",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3"
  }
}
